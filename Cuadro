function setup() {
  createCanvas(400,400);
}
function draw() {
  background(0);
  fill(0,255,0);
  rect(150,150,100,100);
  if(mouseIsPressed){
    if(mouseX<250){
      if(mouseX>150){
        if(mouseY<250){
          if(mouseY>150){
            background(0,255,0);
          }
        }
      }
    }
  }
}
